{
    "name": "alxarafe/workframe",
    "description": "WorkFrame Application",
    "type": "project",
    "license": "GPL-3.0-or-later",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../alxarafe",
            "options": {
                "symlink": true
            }
        }
    ],
    "require": {
        "php": "^8.2",
        "alxarafe/alxarafe": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "squizlabs/php_codesniffer": "^3.10",
        "phpstan/phpstan": "^2.1",
        "php-debugbar/php-debugbar": "^3.3"
    },
    "config": {
        "audit": {
            "ignore": [
                "PKSA-y2cr-5h3j-g3ys"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Modules\\": "Modules/"
        }
    }
}